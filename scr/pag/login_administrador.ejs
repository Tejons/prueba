<!DOCTYPE html>
<html lang="es">
	<head>
		<title>AcaNunciate</title>
		<%include parches/meta%>
    </head>
    <body class="textura_ladrillo">
     <div id="cont_general">
        <div id="">
        <header class="textura_ladrillo">
            <div id="cont_logo_login_usuario">
               <a href="/">
                  <img src="/imagenes/acalogofinalSFF-4.png">
               </a>      
            </div>
         </header> 
         </div>    
        <div id="cont_general_login">       
          <div id="cont_login">
          <div id="login">
            <div id="titulo_login">
              <h1>Ingrese a su cuenta</h1>
              <hr>
            </div>
                  <div id="cont_alerta_login">
                  <%if(msn.length > 0){%>
                  <div class="error">
                  <%=msn %>
                  </div>
                  <%}%>
             </div>
                <div id="cajas_login">
                    <form action="/iniciar_sesion_administrador" method="post" >
                        <label for="correo" class="l-cajas">Correo electrónico</label>
                        <input type="email" id="correo" class="cajas" name="correo" required placeholder="Introduce correo">
                        <label for="contrasena" class="l-cajas">Contraseña</label>
                        <input type="password" id="contrasena" class="cajas" name="contrasena" required placeholder="Introduce contraseña">
                        <input type="submit" value="Entrar" id="entrar">
                    </form>
                </div>
           </div>
         </div>
       </div>
     </div>
     <%include parches/footer%>
    </body>
<%include parches/script%>
</html> 